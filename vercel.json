{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "python3.11",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },

    {
      "src": "/reports/(.*)",
      "dest": "/core/resources/$1"
    },

    {
      "src": "/(.*)",
      "dest": "/core/resources/index.html"
    }
  ]
}
{
  "version": 3,
  "functions": {
    "api/spotlight.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    {
      "src": "/api/spotlight",
      "dest": "/api/spotlight.py"
    }
  ]
}
